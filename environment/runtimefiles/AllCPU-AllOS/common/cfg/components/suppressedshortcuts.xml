      <IFONE ShortCut="Q" Editable='1'/>
      <IFONE ShortCut="R" Editable='1'/>
      <IFONE ShortCut="S" Editable='1'/>
      <IFONE ShortCut="T" Editable='1'/>
      <IFONE ShortCut="U" Editable='1'/>
      <IFONE ShortCut="W" Editable='1'/>
      <IFONE ShortCut="V" Editable='1'/>
      <IFONE ShortCut="X" Editable='1'/>
      <IFONE ShortCut="Y" Editable='1'/>
      <IFONE ShortCut="Z" Editable='1'/>
      <IFONE ShortCut="1" Editable='1'/>
      <IFONE ShortCut="2" Editable='1'/>
      <IFONE ShortCut="3" Editable='1'/>
    </OTHERCONTROLINFOCUS>
  </SUPRESSEDSHORTCUTS>
</CONFIG>