<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <SUPRESSEDSHORTCUTS>
    <DEFAULTCONTROLINFOCUS>
      <!--Эта ветка работает когда в фокусе контрол поумолчанию - командная строка или едит инспектора объектов-->
      <!--IFONE шорткат будет подавлен при выполнении хотябы одного условия-->
      <!--IFALL шорткат будет подавлен при выполнении всех условий (пока не реализвано)-->
      <!--Возможные условия:-->
                       <!--Default - это контрол поумолчанию, надо это выпилить - выделил в отдельную ветку-->
                       <!--NotEmpty - не пустое содержимое контрола-->
                       <!--WithSelection - в контроле есть выделенный текст-->
                       <!--Editable - контрол который можно редактировать-->
                       <!--выше перечисленное может принимать значения '1'-true '0'-false-->
                       <!--ClassName - шаблон имени класса контрола (пока не реализвано)-->
                       <!--ControlName - шаблон имени контрола (пока не реализвано)-->
                       <!--NotControlNameMask - шаблон имени контрола не совпадает с маской-->
      <IFONE ShortCut="INSERT" NotEmpty='1'/>
      <IFONE ShortCut="SPACE" NotEmpty='1'/>
      <IFONE ShortCut="HOME" NotEmpty='1'/>
      <IFONE ShortCut="END" NotEmpty='1'/>
      <IFONE ShortCut="LEFT" NotEmpty='1'/>
      <IFONE ShortCut="RIGHT" NotEmpty='1'/>
      <IFONE ShortCut="UP" NotEmpty='1'/>
      <IFONE ShortCut="DOWN" NotEmpty='1'/>
      <IFONE ShortCut="DEL" NotEmpty='1'/>
      <IFONE ShortCut="CTRL+INS" NotEmpty='1'/>
      <IFONE ShortCut="SHIFT+INS" NotEmpty='1'/>
      <IFONE ShortCut="CTRL+V" NotEmpty='1'/>
      <IFONE ShortCut="CTRL+C" NotEmpty='1'/>
      <IFONE ShortCut="CTRL+X" NotEmpty='1'/>
      <IFONE ShortCut="CTRL+Z" NotEmpty='1'/>
      <IFONE ShortCut="SHIFT+CTRL+Z" NotEmpty='1'/>
      <IFONE ShortCut="A" Editable='1'/>
      <IFONE ShortCut="B" Editable='1'/>
      <IFONE ShortCut="C" Editable='1'/>
      <IFONE ShortCut="D" Editable='1'/>
      <IFONE ShortCut="E" Editable='1'/>
      <IFONE ShortCut="F" Editable='1'/>
      <IFONE ShortCut="G" Editable='1'/>
      <IFONE ShortCut="H" Editable='1'/>
      <IFONE ShortCut="I" Editable='1'/>
      <IFONE ShortCut="J" Editable='1'/>
      <IFONE ShortCut="K" Editable='1'/>
      <IFONE ShortCut="L" Editable='1'/>
      <IFONE ShortCut="M" Editable='1'/>
      <IFONE ShortCut="N" Editable='1'/>
      <IFONE ShortCut="O" Editable='1'/>
      <IFONE ShortCut="P" Editable='1'/>
      <IFONE ShortCut="Q" Editable='1'/>
      <IFONE ShortCut="R" Editable='1'/>
      <IFONE ShortCut="S" Editable='1'/>
      <IFONE ShortCut="T" Editable='1'/>
      <IFONE ShortCut="U" Editable='1'/>
      <IFONE ShortCut="W" Editable='1'/>
      <IFONE ShortCut="V" Editable='1'/>
      <IFONE ShortCut="X" Editable='1'/>
      <IFONE ShortCut="Y" Editable='1'/>
      <IFONE ShortCut="Z" Editable='1'/>
      <IFONE ShortCut="1" Editable='1'/>
      <IFONE ShortCut="2" Editable='1'/>
      <IFONE ShortCut="3" Editable='1'/>
    </DEFAULTCONTROLINFOCUS>
    <OTHERCONTROLINFOCUS>
      <!--Эта ветка работает когда в фокусе какойто другой контрол - например tedit инспектора обьектов-->
      <IFONE ShortCut="INSERT" Editable='1'/>
      <IFONE ShortCut="SPACE" Editable='1'/>
      <IFONE ShortCut="HOME" Editable='1'/>
      <IFONE ShortCut="END" Editable='1'/>
      <IFONE ShortCut="LEFT" Editable='1' ControlNameMask='*navtree*'/>
      <IFONE ShortCut="RIGHT" Editable='1' ControlNameMask='*navtree*'/>
      <IFONE ShortCut="UP" Editable='1' ControlNameMask='*navtree*'/>
      <IFONE ShortCut="DOWN" Editable='1' ControlNameMask='*navtree*'/>
      <IFONE ShortCut="DEL" Editable='1'/>
      <IFONE ShortCut="CTRL+INS" Editable='1' WithSelection='1'/>
      <IFONE ShortCut="SHIFT+INS" Editable='1'/>
      <IFONE ShortCut="CTRL+V" Editable='1'/>
      <IFONE ShortCut="CTRL+C" Editable='1' WithSelection='1'/>
      <IFONE ShortCut="CTRL+X" Editable='1' WithSelection='1'/>
      <IFONE ShortCut="CTRL+Z" Editable='1'/>
      <IFONE ShortCut="SHIFT+CTRL+Z" Editable='1'/>
      <IFONE ShortCut="A" Editable='1'/>
      <IFONE ShortCut="B" Editable='1'/>
      <IFONE ShortCut="C" Editable='1'/>
      <IFONE ShortCut="D" Editable='1'/>
      <IFONE ShortCut="E" Editable='1'/>
      <IFONE ShortCut="F" Editable='1'/>
      <IFONE ShortCut="G" Editable='1'/>
